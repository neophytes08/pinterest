<div id="pins">
  <div class="transitions-enabled">
  <% @pins.each do |pin| %>
    <div class="box">
      <div class="panel">
        <div class="panel-default">
        <%= link_to (image_tag pin.image.url), pin %>
          <div class="panel-body">
            <h2><%= link_to pin.title, pin %></h2>
            <p class="user">
              Submitted by <%= pin.user.email %>
            </p>
          </div>
        </div>
      </div>
    </div>
  <% end %>
  </div>
</div> 