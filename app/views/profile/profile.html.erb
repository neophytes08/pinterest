<div class="panel panel-default">
    <div class="panel panel-body">
      <div class="row text-center">
          <img src="data:image/png;base64," class="img-circle" alt="Cinque Terre" width="300" height="300">

          <h3><%= @user.user_info.fname %> <%= @user.user_info.lname %></h3>
          <h4><%= link_to @user.pins.count  %> Post</h4>
          <h5>Lives at <%= @user.user_info.address %></h5>
          <h5>Born <%= @user.user_info.bday.strftime("%B, %d, %Y") %></h5>
      </div>
    </div>
</div>